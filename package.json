{
  "name": "DariaKlover-website",
  "version": "2.3.0",
  "private": true,
  "description": "Daria Klover's personal website sources",
  "author": "Siarhei Bobryk",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/SiarheiBobryk/dariaklover.git"
  },
  "scripts": {
    "build": "nx build",
    "e2e:ci": "nx e2e website-e2e --configuration=ci",
    "e2e": "nx e2e website-e2e --configuration=production",
    "format:fix": "prettier . --write",
    "format": "prettier . --check",
    "lint:fix": "nx run-many --target=lint --fix",
    "lint": "nx run-many --target=lint",
    "nx:upgrade": "yarn up '@nx/*' nx -E",
    "prepare": "husky install",
    "start": "nx serve",
    "test:ci": "nx run-many --target=test --ci",
    "test": "nx run-many --target=test --watch",
    "type:check": "tsc --project apps/website/tsconfig.app.json --noEmit",
    "type:check:e2e": "tsc --project apps/website-e2e/tsconfig.json --noEmit",
    "type:check:types": "tsc --project libs/types/tsconfig.lib.json --noEmit"
  },
